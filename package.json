{
	"id": "xcf.mini.cd",
	"name": "mini-cd",
	"description": "微信小程序自动化上传",
	"displayName": "微信小程序一键打包并上传到微信后台",
	"version": "0.0.3",
	"publisher": "coderxcf",
	"engines": {
		"HBuilderX": "^2.7.0"
	},
	"categories": [
		"Other"
	],
	"main": "./lib/extension",
	"activationEvents": [
		"onCommand:xcf.mini.cd.upload",
		"onCommand:xcf.mini.cd.submit"
	],
	"contributes": {
		"commands": [{
			"command": "xcf.mini.cd.submit",
			"title": "上传微信小程序"
		},{
			"command": "xcf.mini.cd.upload",
			"title": "一键上传微信小程序"
		}],
		"menus": {
			"menubar/publish": [
				{
					"command": "xcf.mini.cd.submit",
					"group": "0_foot",
					"when": "workspaceFolder.type == 'UniApp_Vue'"
				},
				{
					"command": "xcf.mini.cd.upload",
					"group": "0_foot",
					"when": "workspaceFolder.type == 'UniApp_Vue'"
				}
			]
		}
	},
	"extensionDependencies": [
		"plugin-manager"
	],
	"dependencies": {}
}
